{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.compilerPath": "/usr/bin/arm-none-eabi-gcc",
    "C_Cpp.files.exclude": {
        "/bin": true,
        "/boot": true,
        "/cdrom": true,
        "/dev": true,
        "/proc": true,
        "/etc":true,
        "**/.git": true,
        "pico-sdk/lib": true,
        "pico-sdk/test": true,
        "pico-sdk/tools": true,
        "build": true,
    },
    "cortex-debug.openocdPath": "/usr/local/bin/openocd",
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "git.detectSubmodules": false,
    "git.detectSubmodulesLimit": 70,
    "files.associations": {
        "main.h": "c",
        "queue.h": "c",
        "freertos.h": "c",
        "ssd1306.h": "c",
        "font.h": "c",
        "image.h": "c",
        "adc.h": "c"
    },
}